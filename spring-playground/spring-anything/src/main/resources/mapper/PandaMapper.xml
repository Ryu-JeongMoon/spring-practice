<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springanything.mybatis.PandaMapper">
  <insert id="save" parameterType="com.springanything.jpa.Panda" useGeneratedKeys="true">
    insert into panda (name, age)
    values (#{name}, #{age})
  </insert>
</mapper>
